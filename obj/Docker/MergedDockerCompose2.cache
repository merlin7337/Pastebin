name: dockercompose11897508586898342573
services:
  pastebin:
    build:
      context: F:\Rider projects\Pastebin
      dockerfile: Pastebin/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: pastebin
    networks:
      pastebinNetwork: null
